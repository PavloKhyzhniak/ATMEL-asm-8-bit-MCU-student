;**************************************
; тест МК для точной подстройки частоты
; организуем меандр в 20 тактов
; 10 тактов высокий уровень
; 10 тактов низкий уровень
test_main:
	cli
	ldi r16, 0xFF
	out DDRB, r16
	out PORTB, r16
	nop
	nop
	nop
	nop
	nop
	nop
	ldi r16, 0xFF
	out DDRB, r16
	clr r16
	out PORTB, r16
	nop
	nop
	nop
	nop
		rjmp test_main
;**************************************
